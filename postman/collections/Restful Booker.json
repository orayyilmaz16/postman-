		{
			"name": "CreateBooking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "d137196e-6fb7-4c95-bb63-8d301fc8c20f",
						"exec": [
							"console.clear();\r",
							"\r",
							"const firstName = pm.variables.replaceIn(\"{{$randomFirstName}}\");\r",
							"pm.collectionVariables.set(\"firstName\", firstName);\r",
							"\r",
							"const  lastName = pm.variables.replaceIn(\"{{$randomLastName}}\");\r",
							"pm.collectionVariables.set(\"lastName\",lastName);\r",
							"\r",
							"const depositPaid = pm.variables.replaceIn(\"{{$randomBoolean}}\");\r",
							"pm.collectionVariables.set(\"depositPaid\",depositPaid);\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "a4b46584-bfbc-4a39-98ca-4110414c8d6b",
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const jsonData = pm.response.json();\r",
							"\r",
							"pm.test(\"firstname\" , function(){\r",
							"    pm.expect(jsonData.booking.firstName).to.eql(pm.collectionVariables.get(\"$firstName\"));\r",
							"});\r",
							"\r",
							"pm.test(\"lastname\", function(){\r",
							"    pm.expect(jsonData.booking.lastname).to.eql(pm.collectionVariables.get(\"lastName\"));\r",
							"})\r",
							"\r",
							"pm.test(\"Test data type of the response\", () =>{\r",
							"    pm.expect(jsonData.booking.firstName).to.not.be.null;\r",
							"    pm.expect(jsonData.booking.depositpaid).to.not.be.undefined;\r",
							"});\r",
							"\r",
							"pm.collectionVariables.unset(\"fakedepositpaid\");\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
